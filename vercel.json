{
  "version": 2,
  "builds": [
    {
      "src": "api/chat.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/chat",
      "dest": "/api/chat.py"
    },
    {
      "src": "/health",
      "dest": "/api/chat.py"
    },
    {
      "src": "/",
      "dest": "/api/chat.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/chat.py"
    }
  ],
  "functions": {
    "api/chat.py": {
      "maxDuration": 30
    }
  }
}
